{
	"name": "webflorist/privacy-policy-laravel",
	"description": "A Privacy Policy Package for Laravel Applications. Supports german and english languages.",
	"keywords": [
		"privacy",
		"policy",
		"privacy-policy",
		"data-protection",
		"localization",
		"legal",
		"german",
		"english",
		"gdpr",
		"laravel"
	],
	"homepage": "https://github.com/webflorist/privacy-policy-laravel#readme",
	"support": {
		"issues": "https://github.com/webflorist/privacy-policy-laravel/issues",
		"source": "https://github.com/webflorist/privacy-policy-laravel"
	},
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Gerald Buttinger",
			"email": "gerald@code.florist"
		}
	],
    "require": {
        "laravel/framework": ">=5.5 <9.0.0",
		"webflorist/privacy-policy-text": "^1.3"
    },
	"require-dev": {
		"php": "^7.4|^8.0",
        "orchestra/testbench": ">=3.5 <7.0.0"
	},
    "autoload": {
        "psr-4": {
            "Webflorist\\PrivacyPolicy\\": "src/PrivacyPolicy/",
            "PrivacyPolicyTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\PrivacyPolicy\\PrivacyPolicyServiceProvider"
            ],
            "aliases": {
                "PrivacyPolicy": "Webflorist\\PrivacyPolicy\\Facades\\PrivacyPolicy"
            }
        }
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
